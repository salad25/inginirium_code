#include <iostream>
#include <conio.h>
#include <vector>
using namespace std;
int main()
{
    int a[8]={1, 2, 3, 4, 5, 6, 7, 8};
	vector<int> vec(a, a+8);
    vec.push_back(9);
    vec.push_back(10);
    vector<int>::reverse_iterator it;
    for(it=vec.rbegin(); it!=vec.rend(); it++)  
    {
        cout << *it << endl;	
	}  
    getch();
	return 0;    
	
}
